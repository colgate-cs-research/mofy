!
version 12.4
!
hostname C
!
ip access-list extended aruba-mgmt
 permit ip 10.41.2.0 0.0.0.127 any
 permit tcp host 149.43.132.46 10.41.2.1 0.0.0.127 eq 22
 deny tcp host 149.43.132.200 10.41.2.0 0.0.127.255 eq 22
 permit tcp host 149.43.132.201 10.41.2.0 0.0.0.127 eq 22
!
interface GigabitEthernet0/1
    description INFRA:B:Gi0/1
    ip address 1.0.2.1 255.255.128.0
    ip ospf cost 1
!
interface GigabitEthernet0/2
    description INFRA:D:Gi0/2
    ip address 2.0.2.2 255.255.128.0
    ip ospf cost 3
!
interface GigabitEthernet0/3
    ip address 13.0.2.3 255.128.0.0
!
router bgp 11
    network 11.0.0.0/9
    neighbor 1.0.2.1 remote-as 13
    neighbor 3.0.3.1 remote-as 12
!
  address-family ipv4
  network 149.43.0.0/17
  exit-address-family
!
end
